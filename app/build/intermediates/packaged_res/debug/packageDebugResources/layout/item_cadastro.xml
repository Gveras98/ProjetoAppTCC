<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="8dp" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textViewCadastroNome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Nome: "/>

        <TextView
            android:id="@+id/textViewCadastroDataNascimento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Data Nascimento: "/>

        <TextView
            android:id="@+id/textViewCadastroCPF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="CPF: "/>

        <TextView
            android:id="@+id/textViewCadastroEndereco"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Endereço: "/>

        <TextView
            android:id="@+id/textViewCadastroCEP"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Cep: "/>

        <TextView
            android:id="@+id/textViewCadastroTelefone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Telefone "/>

        <TextView
            android:id="@+id/textViewCadastroEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="email: "/>

        <TextView
            android:id="@+id/textViewCadastroNumeroSUS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="N° Sus: "/>

        <TextView
            android:id="@+id/textViewCadastroPlanoSaude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Plano de Saúde: "/>

        <TextView
            android:id="@+id/textViewCadastroNumeroPlanoSaude"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="N° Plano de Saúde: "/>

        <TextView
            android:id="@+id/textViewCadastroTipoSanguineo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:textSize="18sp"
            tools:text="Tipo Sanguineo: "/>

    </LinearLayout>

</androidx.cardview.widget.CardView>